<h1>Users</h1>
<div>
  <%= form_for :user_search_form, url: users_path, method: :post do |f| %>
    <%= f.label 'Query' %>
    <%= f.text_field :query %>
    <%= f.submit 'Search' %>
  <% end %>
</div>
<% if @users.any? %>
    <ul>
    <% @users.each do |user| %>
        <li><%= user.first_name %> <%= user.last_name %></li>
    <% end %>
    </ul>
<% end %>